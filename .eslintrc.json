{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier", "react", "jsx-a11y", "import"],
  "rules": {
    "prettier/prettier": ["error"],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/jsx-one-expression-per-line": "off"
  },
  "env": {
    "browser": true,
    "node": true
  }
}
